<script>
$(function() {
	$('form#dcore-add-to-cart-3685').each(function() {
		var form = $(this);
		var rtdp = parseInt(form.attr('data-shop-currency-rtdp'));
		var submit = form.find('button[type="submit"]');
		var wishlist = form.find('button.add-to-wishlist');
			var buynow = form.find('button.buy-now');
			var outofstock = form.find('button.out-of-stock');
		var options = form.find('select.f-option').change(checkStock).change(function () {
			var allOptionsSelected = true;
			$(options).each(function () {
				var option = $(this);
				if (!$(this).val()) {
					allOptionsSelected = false;
				}
			});
			wishlist.prop('disabled', allOptionsSelected ? false : true);
					buynow.prop('disabled', allOptionsSelected ? false : true);
				});
		var quantity = form.find('[name="quantity"]').change(checkStock);
			var giftWrap = form.find('[name="gift_wrap"]').change(checkStock);
			var buttons = form.find('.buttons');
		var totalPrice = form.find('.total-price');

		submit.bind('check-instock', function(e) {
			var blockscreen = $('<div />')
				.addClass('block-screen')
				.attr('unselectable', 'on')
				.on('selectstart', false)
				.appendTo(form);
			var loading = $('<div />')
				.addClass('loading')
				.appendTo(buttons)
				.append($('<div />').addClass('spinner').spin())
				.append($('<div />').addClass('screen'));
			$.dpost('/shop/flat-u2-26/variation/isinstock', form.serialize(), function(data) {
				blockscreen.remove();
				loading.remove();
				var stock = parseInt(data.stock);
				form.find('input[name="uid_variation"]').val(data.uid_variation);

				if (isNaN(quantity.val())) {
					quantity.val(1);
				}

							if (!isNaN(stock) && stock > 0) {
					if (stock >= parseInt(quantity.val())) {
											buynow.prop('disabled', false).removeClass('hide');
											submit.dformLoaded().removeClass('hide');
						outofstock.addClass('hide');
					} else {
											buynow.prop('disabled', true).removeClass('hide');
											submit.dformLoading().removeClass('hide');
						outofstock.addClass('hide');
						$.dtoast('재고량이 부족합니다.');
					}
				} else {
					buynow.prop('disabled', true).addClass('hide');
					submit.dformLoading().addClass('hide');
					outofstock.removeClass('hide');
					$("#naver_pay").hide();
				}
						}, 'json');
		});

		function checkStock() {
			var disabled = false;

			if (isNaN(quantity.val()) || !quantity.val().match(/^[1-9]{1}[0-9]*$/)) {
				quantity.val(1);
			}

			if (options.find('option:selected:disabled').length > 0) {
				disabled = true;
			}

			if (disabled) {
				submit.dformLoading().find('.fa').removeClass('fa-spinner fa-spin');
			} else {
				submit.trigger('check-instock');

				var price = 590000;
				options.find('option:selected').each(function() {
					var option = $(this);
					var optionPrice = option.attr('data-price');
					if (optionPrice) {
						price += parseFloat($(this).attr('data-price'));
					}
				});
							if (giftWrap.is(':checked')) {
					price += parseFloat(giftWrap.attr('data-cost'));
				}
							price *= parseInt(quantity.val());
				if (price < 0) {
					price = 0;
				}
				totalPrice.removeClass('hide').find('.dform-currency').text(price).dformCurrency({ roundToDecimalPlace: rtdp });
			}
		}

		if (options.length == 0) {
			checkStock();
		}

		var addToWishlist = false;
		wishlist.click(function (e) {
			e.preventDefault();
			addToWishlist = true;
			form.submit();
		});

			var buyNow = false;
		buynow.click(function (e) {
			e.preventDefault();
			buyNow = true;
			form.submit();
		});
	
		form.dform({
			submit: function() {
				if (submit.dformIsLoading()) {
					return false;
				}
				submit.dformLoading();
				wishlist.prop('disabled', true);
							buynow.prop('disabled', true);
							var action = form.attr('action');
				if (addToWishlist) {
					action = '/api/wishlist/create';
							} else if (buyNow) {
					action = '/api/buynow/create';
							}
				$.dpost(action, form.serialize(), function(data) {
					submit.dformLoaded().blur();
					wishlist.prop('disabled', false).blur();
					if (addToWishlist) {
											ga('send', 'event', 'wishlist', 'add', '위시리스트에 추가');
																addToWishlist = false;
						if (!data.error) {
							$.pjaxRedirect('/user/wishlist');
							return;
						}
									} else if (buyNow) {
						buyNow = false;
						if (!data.error) {
							$.pjaxRedirect('/user/buynow');
							return;
						}
									}
					if (data.error) {
						$.dtoast(data.error);
						return;
					}
									ga('send', 'event', 'cart', 'add', '장바구니에 추가', 3685);
													$(document).trigger('update-cart', true);
					if ($.isPhoneAndTablet()) {
						$.pjaxRedirect('/user/cart');
					}
				}, 'json');
				return false;
			}
		});
	});
});
</script>
